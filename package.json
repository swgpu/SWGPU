    "vite-plugin-wasm": "^3.4.1"
  },
  "dependencies": {
    "@webgpu/types": "^0.1.21",
    "box2d-wasm": "^7.0.0",
    "curve-interpolator": "^3.3.1",
    "dotenv": "^16.4.5",
    "esbuild": "^0.25.0",
